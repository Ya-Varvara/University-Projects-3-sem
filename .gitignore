# Ignore bundler config.
Laba_8/.bundle
Laba_8/first/.bundle

# Ignore the default SQLite database.
Laba_8/db/*.sqlite3
Laba_8/db/*.sqlite3-journal
Laba_8/db/*.sqlite3-*

Laba_8/first/db/*.sqlite3
Laba_8/first/db/*.sqlite3-journal
Laba_8/first/db/*.sqlite3-*
# Ignore all logfiles and tempfiles.
Laba_8/log/*
Laba_8/tmp/*

Laba_8/first/log/*
Laba_8/first/tmp/*

# Ignore pidfiles, but keep the directory.
Laba_8/tmp/pids/*
!Laba_8/tmp/pids/

Laba_8/first/tmp/pids/*
!Laba_8/first/tmp/pids/

# Ignore uploaded files in development.
Laba_8/storage/*

Laba_8/public/assets
Laba_8/.byebug_history

Laba_8/first/storage/*

Laba_8/first/public/assets
Laba_8/first/.byebug_history

# Ignore master key for decrypting credentials and more.
Laba_8/first/config/master.key

Laba_8/first/public/packs
Laba_8/first/public/packs-test
Laba_8/node_modules
Laba_8/yarn-error.log
Laba_8/yarn-debug.log*
Laba_8/.yarn-integrity
!*/.keep

Laba_8/first/node_modules
Laba_8/first/yarn-error.log
Laba_8/first/yarn-debug.log*
Laba_8/first/.yarn-integrity
!*/.keep

